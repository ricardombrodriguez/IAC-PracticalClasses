.data
num:		.space 4
prompt1:	.asciiz "Introduza um número:\n"
result1:	.asciiz "\nO fatorial do número "
result2:	.asciiz " é: "

.text
main:
	la	$a0,prompt1
	li	$v0,4
	syscall
	li	$v0,5
	syscall
	move	$s0,$v0
	la	$t0,num
	sw	$s0,0($t0)
	
	li	$t1,1
	
fact:	
	beqz 	$s0, end
	mul 	$t1,$t1,$s0
	
	addi 	$s0,$s0,-1 
	j	fact
	
end:
	la 	$a0,result1
	li	$v0,4
	syscall
	
	la	$t2,num
	lw	$a0,0($t2)
	li	$v0,1
	syscall
	
	la 	$a0,result2
	li	$v0,4
	syscall
	
	move	$a0,$t1
	li	$v0,1
	syscall
	
	


	
	

	
